{
  "name": "spotify-auth-server",
  "version": "1.0.0",
  "description": "Spotify authentication server with Express",
  "main": "dist/src/get-spotify-auth.js",
  "scripts": {
    "start": "ts-node src/get-spotify-auth.ts",
    "start:prod": "NODE_ENV=production ts-node src/get-spotify-auth.ts",
    "start:prod:win": "set NODE_ENV=production&& ts-node src/get-spotify-auth.ts",
    "build": "tsc",
    "dev": "nodemon --exec ts-node src/get-spotify-auth.ts"
  },
  "dependencies": {
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "open": "^9.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
