{
  "name": "spotify-playlist-viewer",
  "version": "1.0.0",
  "description": "Web application to view Spotify playlists using Spotify API",
  "main": "dist/src/index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "nodemon --exec ts-node src/index.ts",
    "start:prod": "NODE_ENV=production ts-node src/index.ts",
    "start:prod:win": "set NODE_ENV=production&& ts-node src/index.ts",
    "build": "tsc"
  },
  "dependencies": {
    "dotenv": "^16.4.1",
    "express": "^4.21.2",
    "node-fetch": "^3.3.2",
    "open": "^10.1.0",
    "spotify-playlist-viewer": "file:"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.13.14",
    "@types/node-fetch": "^2.6.12",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
